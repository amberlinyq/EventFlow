{
  "name": "eventflow",
  "version": "1.0.0",
  "description": "Production-ready event ingestion and processing system",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "worker": "tsx src/worker.ts",
    "dev:worker": "tsx watch src/worker.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "events",
    "backend",
    "pubsub",
    "postgresql"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/bigquery": "^7.3.0",
    "@google-cloud/pubsub": "^3.5.0",
    "@prisma/client": "^5.7.1",
    "express": "^4.18.2",
    "zod": "^3.22.4",
    "pino": "^8.17.2",
    "pino-http": "^8.5.0",
    "pino-pretty": "^10.3.1",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@types/cors": "^2.8.17",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "prisma": "^5.7.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "prettier": "^3.1.1",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11"
  }
}

